{
    "name": "salainen",
    "version": "0.0.4",
    "providers": {
        "plain": {
            "enabled": true,
            "name": "Plain text",
            "custom": {
            }
        },
        "env": {
            "enabled": true,
            "name": "Environmental Variable",
            "custom": {
                "Prefix": "{{.ProductName}}"
            }
        },
        "prompt": {
            "enabled": true,
            "name": "Prompt",
            "custom": {
                "Prefix": "{{.ProductName}}"
            }
        },
        "file": {
            "enabled": true,
            "name": "File System",
            "custom": {
                "RootPath": "~/.secrets/salainen"
            }
        },
        "efile": {
            "enabled": true,
            "name": "Encrypted File System",
            "custom": {
                "RootPath": "~/.secrets/salainen",
                "Algorithm": ""
            }

        },
        "keyring": {
            "enabled": true,
            "name": "Keyring",
            "custom": {
                "Service": "{{.ProductName}}"
            }
        },
        "wincred": {
            "enabled": false,
            "name": "Windows Credential Manager",
            "custom": {
                "Prefix": "{{.ProductName}}"
            }
        },
        "bitwarden": {
            "enabled": false,
            "name": "Bitwarden",
            "custom": {
                "ApiUrl": "http://localhost:8087",
                "IdentityURL": "",
                "AccessToken" : "keyring:test_BW"
            }
        },
        "keepass": {
            "enabled": false,
            "name": "Keepass",
            "custom": {
                "Path": "",
                "MasterPassword" : "keyring:test_kp",
                "DefaultGroup": "",
                "PrimaryGroup": ""
            }
        }
    }
}

