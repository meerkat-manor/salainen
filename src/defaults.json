{
    "name": "salainen",
    "version": "0.0.1",
    "storage": {
        "env": {
            "enabled": true,
            "name": "Environmental Variable",
            "custom": {
                "Prefix": "{{.ProductName}}"
            }
        },
        "prompt": {
            "enabled": true,
            "name": "Prompt",
            "custom": {
                "Prefix": "{{.ProductName}}"
            }
        },
        "file": {
            "enabled": true,
            "name": "File System",
            "custom": {
                "RootPath": "~/.secrets/salainen"
            }
        },
        "efile": {
            "enabled": true,
            "name": "Encrypted File System",
            "custom": {
                "RootPath": "~/.secrets/salainen",
                "Algorithm": ""
            }

        },
        "wincred": {
            "enabled": true,
            "name": "Windows Credential Manager",
            "custom": {
                "Prefix": "{{.ProductName}}"
            }
        },
        "bitwarden": {
            "enabled": true,
            "name": "Bitwarden",
            "custom": {
                "ApiUrl": "http://localhost:8087",
                "IdentityUrl": "",
                "AccessToken" : "wincred:test_BW"
            }
        }
    }
}

