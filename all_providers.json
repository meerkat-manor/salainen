{
    "name": "salainen",
    "version": "0.0.4",
    "providers": {
        "plain": {
            "enabled": true,
            "name": "Plain text",
            "custom": {
            }
        },
        "env": {
            "enabled": true,
            "name": "Environmental Variable",
            "custom": {
                "Prefix": "{{.ProductName}}"
            }
        },
        "prompt": {
            "enabled": true,
            "name": "Prompt",
            "custom": {
                "Prefix": "{{.ProductName}}"
            }
        },
        "file": {
            "enabled": true,
            "name": "File System",
            "custom": {
                "RootPath": "~/.secrets/salainen/plainfile"
            }
        },
        "efile": {
            "enabled": true,
            "name": "Encrypted File System",
            "custom": {
                "RootPath": "~/.secrets/salainen/efile",
                "Algorithm": ""
            }
        },
        "keyring": {
            "enabled": true,
            "name": "Keyring",
            "custom": {
                "Service": "{{.ProductName}}"
            }
        },
        "wincred": {
            "enabled": true,
            "name": "Windows Credential Manager",
            "custom": {
                "Prefix": "{{.ProductName}}"
            }
        },
        "keepass": {
            "enabled": true,
            "name": "Keepass",
            "provider_type": "Level2",
            "custom": {
                "Path": "tests/data/test_secrets.kdbx",
                "MasterPassword" : "plain:tester01",
                "DefaultGroup": "",
                "PrimaryGroup": ""
            }
        },
        "bitwarden": {
            "enabled": true,
            "name": "Bitwarden",
            "provider_type": "Level2",
            "custom": {
                "ApiUrl": "http://localhost:8087",
                "IdentityURL": "",
                "AccessToken" : "keyring:test_BW"
            }
        },
        "vault": {
            "enabled": true,
            "name": "HashiCorp Vault",
            "provider_type": "Level2",
            "custom": {
                "ApiUrl": "http://127.0.0.1:8200",
                "AccessToken" : "plain:salainen-root",
                "Vault": "secret",
                "ElementName": "password"
            }
        }
    }
}

